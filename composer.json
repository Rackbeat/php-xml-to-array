	"description": "Convert XML into arrays",
	"keywords": [
		"xml",
		"php",
		"laravel",
		"array",
		"xml to array"
	],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Lasse Rafn",
			"email": "lasserafn@gmail.com"
		},
		{
			"name": "Rackbeat",
			"email": "open-source@rackbeat.com"
		}
	],
	"require": {
		"php": ">=7.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^7.0",
		"satooshi/php-coveralls": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"Rackbeat\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
